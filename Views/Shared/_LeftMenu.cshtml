                                    <li>
                                        <a id="WifiUsage" href="@Url.Action("WifiUsage", "Admin")">WiFi Usage</a>
                                    </li>
                                    <li>
                                        <a id="WifiReport" href="@Url.Action("WifiReport", "Admin")">WiFi Report</a>
